{
  "cli": {
    "version": ">= 16.18.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "tester": {
      "android": {
        "buildType": "apk",
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true
      },
      "channel": "tester",
      "env": {
        "NODE_VERSION": "20"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
